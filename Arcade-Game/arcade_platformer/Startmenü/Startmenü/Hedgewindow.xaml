<Window x:Class="Startmenü.Hedgewindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Startmenü"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="Hedgewindow" Height="300" Width="600">
    <Grid>
        <Label Content="Please save your new character selection!" HorizontalAlignment="Center" Height="36" VerticalAlignment="Top" Width="498" FontFamily="Arial Black" FontSize="20" Margin="0,57,0,0"/>
        <Button Content="Ok" HorizontalAlignment="Center" Height="23" Margin="0,183,0,0" VerticalAlignment="Top" Width="104" Click="Button_Click"/>

    </Grid>
</Window>
