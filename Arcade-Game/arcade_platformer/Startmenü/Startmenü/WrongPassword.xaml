<Window x:Class="Startmenü.WrongPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Startmenü"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="WrongPassword" Height="300" Width="500">
    <Grid>
        <Label Content="Wrong Password" HorizontalAlignment="Center" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="36" Margin="0,99,0,0"/>
        <Button Name="Closebut" Content="Close" HorizontalAlignment="Center" Height="42" Margin="0,216,0,0" VerticalAlignment="Top" Width="157" Click="Closebut_Click" FontFamily="Arial Black" FontSize="16"/>

    </Grid>
</Window>
